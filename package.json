{
  "name": "@charlietango/ui",
  "description": "Collection of shared UI elements for Charlie Tango projects",
  "version": "0.9.1",
  "author": "Charlie Tango",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/charlie-tango/ui.git"
  },
  "keywords": [
    "emotion",
    "styled-system",
    "ui"
  ],
  "files": [
    "dist/*"
  ],
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "esmodule": "dist/index.modern.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "run-s storybook",
    "dev:watch": "microbundle --watch --jsx React.createElement -f modern,es,cjs --no-compress",
    "build": "run-s build:*",
    "build:bundle": "microbundle --jsx React.createElement -f modern,es,cjs --no-compress",
    "build:copy": "node tools/build-copy",
    "now-build": "run-s build-storybook",
    "lint": "eslint . --ext js,ts,tsx",
    "storybook": "start-storybook -p 9000 -s public",
    "build-storybook": "build-storybook -s public -o build",
    "pretty": "prettier '**/*.{js,ts,tsx,md,json,yml,html,mdx}' --write",
    "version": "run-p build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit true && lint-staged",
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md,mdx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "modulePathIgnorePatterns": [
      "/dist/"
    ]
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@charlietango/use-client-hydrated": "^1.8.2",
    "@charlietango/use-focus-trap": "^1.2.11",
    "@emotion/is-prop-valid": "^0.8.8",
    "@styled-system/css": "^5.1.4",
    "react-remove-scroll": "^2.3.0",
    "styled-system": "^5.1.4",
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0-next.12",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/react": "^11.0.0-next.13",
    "@storybook/addon-essentials": "^6.0.4",
    "@storybook/addons": "^6.0.4",
    "@storybook/react": "^6.0.4",
    "@storybook/theming": "^6.0.4",
    "@testing-library/dom": "^7.22.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__css": "^5.0.13",
    "@types/styled-system__should-forward-prop": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.6.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "globby": "^11.0.1",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "lint-staged": "^10.2.11",
    "microbundle": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "ts-loader": "^8.0.2",
    "typescript": "~3.9.7"
  }
}
